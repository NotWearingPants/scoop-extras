{
	"version": "2017-12-04",
	"homepage": "https://forums.getpaint.net/topic/7291-pyrochild-plugins",
	"description": "pyrochild Plugins for paint.net",
	"depends": "paint.net",
	"url": "https://forums.getpaint.net/applications/core/interface/file/attachment.php?id=13067#/dl.zip",
	"hash": "dc05dd0604d64481ce44d35156a27ec2c6fb53b4a902aeb89cfe521cb0ef699c",
	"installer": {
		"script": [
			"(Get-ChildItem \"$dir\\*.dll\").Name | Out-File \"$dir\\plugins.txt\"",
			"Move-Item \"$dir\\*.dll\" \"$(appdir paint.net)\\current\\Effects\\\""
		]
	},
	"uninstaller": {
		"script": [
			"Get-Content \"$dir\\plugins.txt\" | ForEach-Object { Remove-Item \"$(appdir paint.net)\\current\\Effects\\$_\" }"
		]
	}
}
